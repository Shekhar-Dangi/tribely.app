{
  "expo": {
    "name": "tribely",
    "slug": "tribely",
    "version": "6.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "tribely",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "6.0.0",
      "bundleIdentifier": "com.shekhardangi.tribely",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.shekhardangi.tribely",
      "versionCode": 13
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow tribely to use your location to show relevant content and improve your experience.",
          "locationWhenInUsePermission": "Allow tribely to use your location to show relevant content and improve your experience.",
          "isIosBackgroundLocationEnabled": false,
          "isAndroidBackgroundLocationEnabled": false,
          "isAndroidForegroundServiceEnabled": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
       [
        "expo-audio",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "05c66ce2-a15f-4f4e-983c-aab2e0f6bd35"
      }
    },
    "owner": "shekhardangi"
  }
}
